
package languish;

public class DoubleFunction extends Function {
    
//     public Type getReturnType() {
// 	return Type.INTEGER;
//     }

//     public Type getArgumentType() {
// 	return Type.INTEGER;
//     }
    
    public LObject apply(LObject argument) {
	return new LInteger(2*((LInteger)argument).intValue());
    }
    
}